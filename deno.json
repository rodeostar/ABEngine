{
  "tasks": {
    "run": "deno run --allow-net --allow-read --allow-env --allow-write --allow-run --watch ./app/server.tsx",
    "ws": "deno run --allow-net --allow-read --allow-env --allow-write --allow-run --watch ./app/wss.ts",
    "test": "deno test --allow-net --allow-read --allow-env"
  },
  "imports": {
    "@/": "./",
    "preact": "npm:preact@10.19.3",
    "preact/hooks": "npm:preact@10.19.3/hooks",
    "preact-render-to-string": "npm:preact-render-to-string@6.3.1",
    "tw": "npm:twind@0.16.19",
    "tw/sheets": "npm:twind@0.16.19/sheets",
    "oak": "npm:@oakserver/oak@14.1.0"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"],
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment"
  },
  "nodeModulesDir": "auto"
