{
  "tasks": {
    "dev": "deno run --allow-net --allow-read --allow-env --allow-write --allow-run --watch ./app/server.tsx",
    "test": "deno test --allow-net --allow-read --allow-env",
    "kill": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/kill-ports.ps1"
  },
  "imports": {
    "@/": "./",
    "preact": "npm:preact@10.19.3",
    "preact/hooks": "npm:preact@10.19.3/hooks",
    "preact-render-to-string": "npm:preact-render-to-string@6.3.1",
    "tw": "npm:twind@0.16.19",
    "tw/sheets": "npm:twind@0.16.19/sheets",
    "jsr:@std/yaml": "jsr:@std/yaml@1.0.0",
    "jsr:@std/path": "jsr:@std/path@1.0.0"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "dom.asynciterable", "deno.ns"],
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment"
  },
  "nodeModulesDir": "auto"
}
